RM = rm -f

CC ?= gcc

CFLAGS +=   -ansi                       \
            -pedantic                   \
            -Wall                       \
            -ffloat-store               \
            -fsigned-char               \
            -O                          \
            -DUNIX -I/usr/X11R6/include

debug :     CFLAGS += -g

COMPILE = $(CC) -c $(CFLAGS)

include Makefile.EX
