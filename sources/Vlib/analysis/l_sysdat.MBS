 beginmodule

   lst_ini(get_tstr(1546));        ! ANALYS - Systemdata

   lst_lin(get_tstr(1547) + act_varkon_vers());              ! Version
   lst_lin(get_tstr(1548) + str(act_varkon_serial(),-1,0));  ! Serienummer
   lst_lin(get_tstr(1549) + act_ostype());                   ! OS-typ
   lst_lin(get_tstr(1550) + act_host());                     ! Nodnamn

   lst_exi();

 endmodule

